    if ($('[id^="email-subscribe-block-container"]').length > 0) {
        $('[id^="email-subscribe-block-container"]').each(function () {
            var containerId = this.id;
            var blockId = containerId.substring(31);
            return new window.Vue({
                el: "#" + containerId,
                data: {
                    emailSubscribeErrorMessage: "",
                    emailSubscribeEmail: "",
                    emailSubscribeFirstName: "",
                    emailSubscribeLastName: "",
                    emailSubscribeZipCode: ""
                },
                methods: {
                    submitSubscribeForm: function(event) {
                        event.preventDefault();
                        var self = this;
                        $("#email-subscribe-block-form"+blockId).validate();
                        if ($("#email-subscribe-block-form" + blockId).valid()) {
                            $("#btnEmailSignUp"+blockId+" span").hide();
                            $("#btnEmailSignUp" + blockId + " .fa-spin").css("display", "inline-block");
                            $("#btnEmailSignUp" + blockId).css("opacity", ".65");
                            app.services.subscribeEmail
                                .submitSubscribe(self.emailSubscribeFirstName,
                                    self.emailSubscribeLastName,
                                    self.emailSubscribeEmail,
                                    self.emailSubscribeZipCode)
                                .then(self.submitSubscribeSuccess)
                                .fail(self.submitSubscribeFailed);
                        }
                    },
                    submitSubscribeSuccess: function(response) {
                        var self = this;
                        self.emailSubscribeErrorMessage = "";
                        self.emailSubscribeEmail = "";
                        self.emailSubscribeFirstName = "";
                        self.emailSubscribeLastName = "";
                        self.emailSubscribeZipCode = "";
                        $("#btnEmailSignUp" + blockId + " span:not(.success-msg)").hide();
                        $("#btnEmailSignUp" + blockId + " span.success-msg").css("display", "inline-block");
                        $("#btnEmailSignUp" + blockId).css("background", "#333333");
                        $("#btnEmailSignUp" + blockId).css("opacity", "1");
                        $("#btnEmailSignUp" + blockId + " .fa-spin").hide();
                    },
                    submitSubscribeFailed: function (response) {
                        $("#btnEmailSignUp" + blockId + " span").css("display", "inline-block");
                        $("#btnEmailSignUp" + blockId +" .fa-spin").hide();
                        var self = this;
                    }
                }
            });
		});
	}

