    <script>
    window.onload = getGeolocation();

    function getGeolocation() {       
        var geolocationCookie = getCookie("is-html-geolocation");
        if (!geolocationCookie) {
            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(htmlGeolocation, defaultGeolocation);
            } else {
                defaultGeolocation();
            }
        }
    }

    function htmlGeolocation(position) {
        EventBus.$emit("acceptedGeolocation");
        deleteGeolocationCookies();
        var latitude = position.coords.latitude;
        var longitude = position.coords.longitude;
        $.ajax({
            type: "POST",
            url: '/UserGeolocation/SetHtmlGeolocationCookies',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({ latitude: latitude, longitude: longitude }),
            success: function () {
                reverseGeocodeLocationIq(latitude, longitude);
                var storeId = getCookie("lsf-my-store-id");
                EventBus.$emit("update-geolocation", { storeId: storeId });
                setCookie("is-html-geolocation", true, 360);
            },
            error: deleteGeolocationCookies()
        });
    }

    function defaultGeolocation() {
        deleteGeolocationCookies();
        $.ajax({
            type: "POST",
            url: '/UserGeolocation/SetDefaultGeolocationCookies',
            contentType: "application/json; charset=utf-8",
            success: function () {
                var storeId = getCookie("lsf-my-store-id");
                EventBus.$emit("update-geolocation", { storeId: storeId });
                setCookie("is-html-geolocation", false, 360);
            },
            error: deleteGeolocationCookies()
        });
    }

    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = document.cookie;
        try {
            decodedCookie = decodeURIComponent(document.cookie);
        }
        catch (err) {} 
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) === ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) === 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+ d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function deleteGeolocationCookies() {
        deleteCookie("lsf-my-store-id");
        deleteCookie("lsf-autodetected-zip-code");
    }

    function deleteCookie(name) {
        document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
    };
    function reverseGeocodeLocationIq(lat, lng) {
        $.get(apiReverseUrl.replace("{0}", lat).replace("{1}", lng))
            .done(function (response, status) {
                if (status === "success") {
                    if (response.address) {
                        var searchAddressComponents = response.address,
                            searchPostalCode = "";
                        if (searchAddressComponents.postcode) {
                            searchPostalCode = searchAddressComponents.postcode;
                        }

                        if (searchPostalCode && searchPostalCode.length === 5) {
                            var storeId = getCookie("lsf-my-store-id");
                            EventBus.$emit("update-geolocation", { storeId: storeId });

                            setCookie("lsf-autodetected-zip-code", searchPostalCode, 720);
                            app.stores.zipCodeStore.autodetectedZipCodeCookieValue = searchPostalCode;
                        } else {
                            defaultGeolocation();
                        }
                    }
                    else {
                        defaultGeolocation();
                    }
                }
                else {
                    defaultGeolocation();
                }
            })
            .fail(function() {
                defaultGeolocation();
            });
    }

</script>

<script type="text/javascript">
    var apiReverseUrl = decodeHtmlAmpersand('https://us1.locationiq.org/v1/reverse.php?key=6fba7bffe49056&amp;lat={0}&amp;lon={1}&amp;format=json');

    function decodeHtmlAmpersand(url) {
        return url.replace(/&amp;/g, '&');
    }

</script>
    <script src="/bundles/main?v=KMim7gK4dPNPhizTzI8WU6yOfTNkVOtgJYGXSi9p3q81"></script>


