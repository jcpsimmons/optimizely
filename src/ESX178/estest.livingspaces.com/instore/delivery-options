        <script language="javascript">
            jQuery.cachedScript = function (url, options) {
                options = $.extend(options || {}, {
                    dataType: "script",
                    cache: true,
                    url: url
                });
                return jQuery.ajax(options);
            };

            window.addEventListener('load', function () {
                $.cachedScript("https://estest.livingspaces.com/bundles/KoreAi?v=2020.04.29").done(function (script, textStatus) {
                    ChattyMcBotloader.SpawnBotty();
                });

                $('<link/>', {
                    rel: 'stylesheet',
                    type: 'text/css',
                    href: 'https://estest.livingspaces.com/bundles/KoreCss?v=2020.04.29'
                }).appendTo('head');
            }, false);
        </script>

